<% if (req.session.password) { %>
<div class="rounded-md bg-green-800 p-4 mb-8">
  <div class="flex">
    <div class="flex-shrink-0">
      <!-- Heroicon name: solid/information-circle -->
      <svg class="h-5 w-5 text-green-300" xmlns="http://www.w3.org/2000/svg" viewBox="0 0 20 20" fill="currentColor" aria-hidden="true">
        <path fill-rule="evenodd" d="M18 10a8 8 0 11-16 0 8 8 0 0116 0zm-7-4a1 1 0 11-2 0 1 1 0 012 0zM9 9a1 1 0 000 2v3a1 1 0 001 1h1a1 1 0 100-2v-3a1 1 0 00-1-1H9z" clip-rule="evenodd" />
      </svg>
    </div>
    <div class="ml-3 flex-1 md:flex md:justify-between">
      <p class="text-sm text-green-400">Password updated to: <code><%= req.session.password %></code></p>
    </div>
  </div>
</div>
<% } %>
<% if (req.query.err == "CLAIMED") { %>
<div class="rounded-md bg-green-800 p-4 mb-8">
  <div class="flex">
    <div class="flex-shrink-0">
      <!-- Heroicon name: solid/information-circle -->
      <svg class="h-5 w-5 text-green-300" xmlns="http://www.w3.org/2000/svg" viewBox="0 0 20 20" fill="currentColor" aria-hidden="true">
        <path fill-rule="evenodd" d="M18 10a8 8 0 11-16 0 8 8 0 0116 0zm-7-4a1 1 0 11-2 0 1 1 0 012 0zM9 9a1 1 0 000 2v3a1 1 0 001 1h1a1 1 0 100-2v-3a1 1 0 00-1-1H9z" clip-rule="evenodd" />
      </svg>
    </div>
    <div class="ml-3 flex-1 md:flex md:justify-between">
      <p class="text-sm text-green-400">Claimed referral code!</p>
    </div>
  </div>
</div>
<% } %>
<% if (req.query.err == "none") { %>
<div class="rounded-md bg-green-800 p-4 mb-8">
  <div class="flex">
    <div class="flex-shrink-0">
      <!-- Heroicon name: solid/information-circle -->
      <svg class="h-5 w-5 text-green-300" xmlns="http://www.w3.org/2000/svg" viewBox="0 0 20 20" fill="currentColor" aria-hidden="true">
        <path fill-rule="evenodd" d="M18 10a8 8 0 11-16 0 8 8 0 0116 0zm-7-4a1 1 0 11-2 0 1 1 0 012 0zM9 9a1 1 0 000 2v3a1 1 0 001 1h1a1 1 0 100-2v-3a1 1 0 00-1-1H9z" clip-rule="evenodd" />
      </svg>
    </div>
    <div class="ml-3 flex-1 md:flex md:justify-between">
      <p class="text-sm text-green-400">Claimed referral code!</p>
    </div>
  </div>
</div>
<% } %>
<% if (req.query.err == "CANNOTCLAIM") { %>
<div class="rounded-md bg-red-800 p-4 mb-8">
  <div class="flex">
    <div class="flex-shrink-0">
      <!-- Heroicon name: solid/information-circle -->
      <svg class="h-5 w-5 text-red-300" xmlns="http://www.w3.org/2000/svg" viewBox="0 0 20 20" fill="currentColor" aria-hidden="true">
        <path fill-rule="evenodd" d="M18 10a8 8 0 11-16 0 8 8 0 0116 0zm-7-4a1 1 0 11-2 0 1 1 0 012 0zM9 9a1 1 0 000 2v3a1 1 0 001 1h1a1 1 0 100-2v-3a1 1 0 00-1-1H9z" clip-rule="evenodd" />
      </svg>
    </div>
    <div class="ml-3 flex-1 md:flex md:justify-between">
      <p class="text-sm text-red-400">Either you've already claimed a referral code or the code you tried to use was made by you.</p>
    </div>
  </div>
</div>
<% } %>
<% if (req.query.err == "INVALIDCODE") { %>
<div class="rounded-md bg-red-800 p-4 mb-8">
  <div class="flex">
    <div class="flex-shrink-0">
      <!-- Heroicon name: solid/information-circle -->
      <svg class="h-5 w-5 text-red-300" xmlns="http://www.w3.org/2000/svg" viewBox="0 0 20 20" fill="currentColor" aria-hidden="true">
        <path fill-rule="evenodd" d="M18 10a8 8 0 11-16 0 8 8 0 0116 0zm-7-4a1 1 0 11-2 0 1 1 0 012 0zM9 9a1 1 0 000 2v3a1 1 0 001 1h1a1 1 0 100-2v-3a1 1 0 00-1-1H9z" clip-rule="evenodd" />
      </svg>
    </div>
    <div class="ml-3 flex-1 md:flex md:justify-between">
      <p class="text-sm text-red-400">No code was provided or the one you provided was invalid.</p>
    </div>
  </div>
</div>
<% } %>
<!-- This example requires Tailwind CSS v2.0+ -->
<div class="bg-pylex-700 px-4 py-5 border-b border-pylex-600 rounded-t sm:px-6">
  <h3 class="text-lg leading-6 font-medium text-white">Panel credentials</h3>
  <p class="mt-1 text-sm text-pylex-300">Credentials for accessing the Pylex control panel.</p>
</div>
<div class="bg-pylex-700 rounded-b px-4 py-5 sm:px-6">
    <dl>
      <div class="bg-transparent px-4 py-5 sm:grid sm:grid-cols-3 sm:gap-4 sm:px-6">
        <dt class="text-sm font-medium text-white">Username</dt>
        <dd class="mt-1 text-sm text-pylex-300 sm:col-span-2 sm:mt-0"><%= req.session.userinfo.id %></dd>
      </div>
      <div class="bg-transparent px-4 py-5 sm:grid sm:grid-cols-3 sm:gap-4 sm:px-6">
        <dt class="text-sm font-medium text-white">Email</dt>
        <dd class="mt-1 text-sm text-pylex-300 sm:col-span-2 sm:mt-0"><%= req.session.userinfo.email %></dd>
      </div>
      <div class="bg-transparent px-4 py-5 sm:grid sm:grid-cols-3 sm:gap-4 sm:px-6">
        <dt class="text-sm font-medium text-white">Password</dt>
        <dd id="password" class="mt-1 text-sm text-pylex-300 sm:col-span-2 sm:mt-0">Fetching...</dd>
      </div>
    </dl>
</div>
<div class="bg-pylex-700 px-4 py-5 border-b border-pylex-600 rounded-t sm:px-6 mt-8">
  <h3 class="text-lg leading-6 font-medium text-white">Security settings</h3>
  <p class="mt-1 text-sm text-pylex-300">This is useful in situations where your account was compromised or you'd like to delete it.</p>
</div>
<div class="bg-pylex-700 rounded-b px-4 py-5 sm:px-6">
    <dl>
      <div class="bg-transparent px-4 py-5 sm:grid sm:grid-cols-3 sm:gap-4 sm:px-6">
        <dt class="text-sm font-medium text-white">Reset password</dt>
        <dd class="mt-1 text-sm text-pylex-300 sm:col-span-2 sm:mt-0">
          <a href="../regen" onclick="submitForm()" class="transition ease-in-out delay-100 group relative flex w-1/3 justify-center rounded-md border border-transparent bg-pylex-600 py-2 px-4 text-sm font-medium text-white hover:bg-pylex-500 focus:outline-none">
            Reset
          </a>
        </dd>
      </div>
      <div class="bg-transparent px-4 py-5 sm:grid sm:grid-cols-3 sm:gap-4 sm:px-6">
        <dt class="text-sm font-medium text-white">Delete account</dt>
        <dd class="mt-1 text-sm text-pylex-300 sm:col-span-2 sm:mt-0">
          <a href="../account/delete" onclick="submitForm()" class="inline transition ease-in-out delay-100 group relative flex w-1/3 justify-center rounded-md border border-transparent bg-red-500 py-2 px-4 text-sm font-medium text-white hover:bg-red-600 focus:outline-none">
            Confirm
          </a>
        </dd>
      </div>
    </dl>
</div>
<div class="bg-pylex-700 px-4 py-5 border-b border-pylex-600 rounded-t sm:px-6 mt-8">
  <h3 class="text-lg leading-6 font-medium text-white">Referral codes</h3>
  <p class="mt-1 text-sm text-pylex-300">Got a referral code? Claim it here for 250 Coins.</p>
</div>
<div class="bg-pylex-700 rounded-b px-4 py-5 sm:px-6">
<form action="/claim" method="get">
  <input type="text" name="code" id="code" class="transition ease-in-out delay-100 block w-1/4 flex-1 text-white rounded-md bg-pylex-600 border-pylex-500 focus:border-sky-400 focus:ring-sky-400 sm:text-sm" placeholder="Referral code"><br>
        <button type="submit" class="transition ease-in-out delay-100 group relative flex w-auto justify-center rounded-md border border-transparent bg-sky-400 py-2 px-4 text-sm font-medium text-white hover:bg-sky-500 focus:outline-none">
          Claim
        </button>
</form>
</div>
<script>
$(document).ready(function() {
  $.ajax({
    url: "/api/password",
    type: "GET",
    success: function(result) {
      $("#password").text(result.password);
    }
  });
});
</script>
